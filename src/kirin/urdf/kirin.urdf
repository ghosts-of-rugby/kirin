<robot name="kirin">
  <link name="base_link"/>

  <link name="fix_base">
    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 -1.57"/>
      <geometry>
        <mesh filename="package://kirin/resources/light/fix.stl"/>
      </geometry>
      <material name="">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <link name="theta_link">
    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 1.57"/>
      <geometry>
        <mesh filename="package://kirin/resources/light/theta.stl"/>
      </geometry>
      <material name="">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <link name="z_link">
    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 1.57"/>
      <geometry>
        <mesh filename="package://kirin/resources/light/z.stl"/>
      </geometry>
      <material name="">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <link name="r_link">
    <visual name="">
      <origin xyz="0.2 0.0 0.0" rpy="0.0 0.0 1.57"/>
      <geometry>
        <mesh filename="package://kirin/resources/light/r.stl"/>
      </geometry>
      <material name="">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <link name="phi_link">
    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 1.57"/>
      <geometry>
        <mesh filename="package://kirin/resources/light/phi.stl"/>
      </geometry>
      <material name="">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <link name="phi_extend_link">
    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 1.57"/>
      <geometry>
        <mesh filename="package://kirin/resources/light/phi_extend.stl"/>
      </geometry>
      <material name="">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="fix_base"/>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
  </joint>

  <joint name="theta_joint" type="continuous">
    <parent link="fix_base"/>
    <child link="theta_link"/>
    <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>

  <joint name="z_joint" type="prismatic">
    <parent link="theta_link"/>
    <child link="z_link"/>
    <origin xyz="0.0 0.0 0.3" rpy="0.0 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit lower="0.1" upper="0.6" effort="100" velocity="2.0"/>
  </joint>

  <joint name="r_joint" type="prismatic">
    <parent link="z_link"/>
    <child link="r_link"/>
    <origin xyz="0.1 0.0 -0.1" rpy="0.0 0.0 0.0"/>
    <axis xyz="1.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.8" effort="100" velocity="5.0"/>
  </joint>

  <joint name="phi_joint" type="revolute">
    <parent link="r_link"/>
    <child link="phi_link"/>
    <origin xyz="0.4 0.0 -0.1" rpy="0.0 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit lower="-3.14" upper="3.14" effort="10.0" velocity="31.4"/>
  </joint>

  <joint name="phi_extend_joint" type="revolute">
    <parent link="r_link"/>
    <child link="phi_extend_link"/>
    <origin xyz="0.4 0.0 -0.2" rpy="0.0 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit lower="-3.14" upper="3.14" effort="10.0" velocity="31.4"/>
  </joint>

</robot>